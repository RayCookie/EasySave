        <Button x:Name="pause_button" Content="Pause and Play" HorizontalAlignment="Left" Height="28" Margin="810,25,0,0" VerticalAlignment="Top" Width="238" Click="pause_button_Click"/>
        <Label x:Name="test" HorizontalAlignment="Left" Height="102" Margin="810,204,0,0" VerticalAlignment="Top" Width="119"/>
        <Label x:Name="test_labs" HorizontalAlignment="Left" Height="89" Margin="805,217,0,0" VerticalAlignment="Top" Width="176"/>
    </Grid>
</Window>
