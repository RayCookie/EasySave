<Window x:Class="WpfPROJECT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPROJECT"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FFCBCBCB">
        <TextBox x:Name="tType" HorizontalAlignment="Left" Margin="82,74,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="28"/>
        <TextBox x:Name="tName" HorizontalAlignment="Left" Margin="82,126,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="29"/>
        <TextBox x:Name="tSource" HorizontalAlignment="Left" Margin="82,188,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="29"/>
        <TextBox x:Name="tDestination" HorizontalAlignment="Left" Margin="82,261,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="29"/>
        <Button Content="create" HorizontalAlignment="Left" Margin="82,316,0,0" VerticalAlignment="Top" Height="55" Width="104" Click="Button_Click"/>
        <TextBox x:Name="result" HorizontalAlignment="Left" Margin="373,217,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="147" Height="52"/>

    </Grid>
</Window>
